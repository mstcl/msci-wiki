@article{flenner_active_2020,
	title = "Active matter: quantifying the departure from equilibrium",
	volume = "102",
	ISSN = "2470-0045, 2470-0053",
	shorttitle = "Active matter",
	URL = "http://arxiv.org/abs/2004.11925",
	DOI = "10.1103/PhysRevE.102.022607",
	abstract = "Active matter systems are driven out of equilibrium at the level
	            of individual constituents. One widely studied class are systems
	            of athermal particles that move under the combined influence of
	            interparticle interactions and self-propulsions, with the latter
	            evolving according to the Ornstein-Uhlenbeck stochastic process.
	            Intuitively, these so-called active Ornstein-Uhlenbeck particles
	            (AOUPs) systems are farther from equilibrium for longer
	            self-propulsion persistence times. Quantitatively, this is
	            confirmed by the increasing equal-time velocity correlations
	            (which are trivial in equilibrium) and by the increasing
	            violation of the Einstein relation between the self-diffusion and
	            mobility coefficients. In contrast, the entropy production rate,
	            calculated from the ratio of the probabilities of the position
	            space trajectory and its time-reversed counterpart, has a
	            non-monotonic dependence on the persistence time. Thus, it does
	            not properly quantify the departure of AOUPs systems from
	            equilibrium.",
	number = "2",
	urldate = "2023-10-05",
	journal = "Physical Review E",
	author = "Elijah Flenner and Grzegorz Szamel",
	month = aug,
	year = "2020",
	note = "arXiv:2004.11925 [cond-mat]",
	keywords = "Condensed Matter - Soft Condensed Matter, Condensed Matter -
	            Statistical Mechanics",
	pages = "022607",
	file = "arXiv Fulltext PDF:/home/lckdscl/Zotero/storage/G4SEBF7S/Flenner and
	        Szamel - 2020 - Active matter quantifying the departure from
	        equilibrium.pdf:application/pdf;arXiv.org
	        Snapshot:/home/lckdscl/Zotero/storage/BNJ48HNJ/2004.html:text/html",
}

