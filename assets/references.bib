	file = "Stenhammar - 2021 - An Introduction to Motility-Induced Phase
	        Separati.pdf:/home/lckdscl/Zotero/storage/3EFR3UT7/Stenhammar - 2021
	        - An Introduction to Motility-Induced Phase
	        Separati.pdf:application/pdf",
}

